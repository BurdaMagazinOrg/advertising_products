<?php

/**
 * @file
 * Contains tracdelight_client.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function tracdelight_client_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the tracdelight_client module.
    case 'help.page.tracdelight_client':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('tracdelight_client') . '</p>';
      return $output;

    default:
  }
}
